{
  "name": "cash-manager",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "ng serve --host=127.0.0.1",
    "start:dev": "ng serve -c=dev",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "npx mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "npx mochawesome-report-generator tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@angular/animations": "19.2.5",
    "@angular/common": "19.2.5",
    "@angular/compiler": "19.2.5",
    "@angular/core": "19.2.5",
    "@angular/forms": "19.2.5",
    "@angular/material": "19.2.8",
    "@angular/material-date-fns-adapter": "19.2.8",
    "@angular/platform-browser": "19.2.5",
    "@angular/platform-browser-dynamic": "19.2.5",
    "@angular/router": "19.2.5",
    "@angular/service-worker": "19.2.5",
    "chart.js": "4.4.8",
    "cypress-wait-until": "3.0.2",
    "date-fns": "4.1.0",
    "jwt-decode": "4.0.0",
    "ng2-charts": "8.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.6",
    "@angular/cli": "19.2.6",
    "@angular/compiler-cli": "19.2.5",
    "@eslint/js": "9.24.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "cypress": "14.1.0",
    "cypress-multi-reporters": "2.0.5",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-cypress": "4.1.0",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-preset-angular": "14.5.3",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "5.0.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.5.3",
    "stylelint": "16.15.0",
    "stylelint-config-standard-scss": "14.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.26.0"
  }
}
