{
  "name": "cash-manager",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "ng serve --host=127.0.0.1",
    "start:dev": "ng serve -c=dev",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "npx mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "npx mochawesome-report-generator tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@angular/animations": "17.3.7",
    "@angular/common": "17.3.7",
    "@angular/compiler": "17.3.7",
    "@angular/core": "17.3.7",
    "@angular/forms": "17.3.7",
    "@angular/material": "17.3.6",
    "@angular/material-date-fns-adapter": "17.3.6",
    "@angular/platform-browser": "17.3.7",
    "@angular/platform-browser-dynamic": "17.3.7",
    "@angular/router": "17.3.7",
    "@angular/service-worker": "17.3.7",
    "chart.js": "4.4.2",
    "cypress-wait-until": "3.0.1",
    "date-fns": "3.6.0",
    "jwt-decode": "4.0.0",
    "ng2-charts": "6.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.6",
    "@angular/cli": "17.3.6",
    "@angular/compiler-cli": "17.3.7",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.8",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "cypress": "13.8.1",
    "cypress-multi-reporters": "1.6.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "3.0.3",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-preset-angular": "14.0.3",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.2.5",
    "stylelint": "16.4.0",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.4.5"
  }
}
