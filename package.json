{
  "name": "cash-manager",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "16.x || 18.x",
    "npm": "7.x || 8.x"
  },
  "scripts": {
    "start": "ng serve",
    "start:dev": "ng serve -c=dev",
    "build": "ng build",
    "test": "jest --config jest-config.js",
    "test:watch": "npm run test -- --watch",
    "test:e2e": "ng e2e",
    "test:e2e:ci": "ng run CashManager:e2e-ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "npx mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "npx mochawesome-report-generator tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@angular/animations": "15.1.5",
    "@angular/common": "15.1.5",
    "@angular/compiler": "15.1.5",
    "@angular/core": "15.1.5",
    "@angular/forms": "15.1.5",
    "@angular/material": "15.1.5",
    "@angular/platform-browser": "15.1.5",
    "@angular/platform-browser-dynamic": "15.1.5",
    "@angular/router": "15.1.5",
    "chart.js": "4.2.1",
    "cypress-wait-until": "1.7.2",
    "date-fns": "2.29.3",
    "jwt-decode": "3.1.2",
    "ng2-charts": "4.1.1",
    "rxjs": "7.8.0",
    "tailwindcss": "3.2.7",
    "tslib": "2.5.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.6",
    "@angular/cli": "15.1.6",
    "@angular/compiler-cli": "15.1.5",
    "@cypress/schematic": "2.5.0",
    "@ngneat/spectator": "14.0.0",
    "@types/jasmine": "4.3.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "cypress": "12.6.0",
    "cypress-multi-reporters": "1.6.2",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-cypress": "2.12.1",
    "gh-pages": "5.0.0",
    "jasmine-core": "4.5.0",
    "jest": "29.4.3",
    "jest-preset-angular": "13.0.0-next.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.2",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "2.8.4",
    "stylelint": "15.1.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "7.0.1",
    "typescript": "4.9.5"
  }
}
