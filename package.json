{
  "name": "cash-manager",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config jest-config.js",
    "test:e2e": "ng e2e",
    "test:e2e:ci": "ng run CashManager:e2e-ci",
    "test:watch": "npm run test -- --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "yarn run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "npx mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "npx mochawesome-report-generator tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@angular/animations": "13.3.2",
    "@angular/common": "13.3.2",
    "@angular/compiler": "13.3.2",
    "@angular/core": "13.3.2",
    "@angular/forms": "13.3.2",
    "@angular/platform-browser": "13.3.2",
    "@angular/platform-browser-dynamic": "13.3.2",
    "@angular/router": "13.3.2",
    "rxjs": "7.5.5",
    "snyk": "1.900.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.2",
    "@angular/cli": "13.3.2",
    "@angular/compiler-cli": "13.3.2",
    "@cypress/schematic": "1.6.0",
    "@ngneat/spectator": "10.0.1",
    "@types/jasmine": "4.0.3",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.27",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "cypress": "9.5.4",
    "cypress-multi-reporters": "1.5.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "jasmine-core": "4.0.1",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.1",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "2.6.2",
    "stylelint": "14.7.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "3.0.0",
    "typescript": "4.6.3"
  }
}
