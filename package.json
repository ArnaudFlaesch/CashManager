{
  "name": "cash-manager",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "24.x",
    "npm": "11.x"
  },
  "scripts": {
    "start": "ng serve --host=127.0.0.1 -o",
    "start:dev": "ng serve -c=dev -o",
    "build": "ng build",
    "test": "ng test",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass}\" ",
    "lint:styles:fix": "npm run lint:styles -- --fix ",
    "lint:styles:report": "npm run lint:styles -- --formatter json --output-file ./styleLintReport.json",
    "report:merge": "npx mochawesome-merge ./cypress/results/mochawesome-report/*.json > tests-report.json",
    "report:generate": "npx mochawesome-report-generator tests-report.json -o cypress/reports/html",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports/html/screenshots",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@angular/animations": "21.0.0",
    "@angular/common": "21.0.1",
    "@angular/compiler": "21.0.0",
    "@angular/core": "21.0.0",
    "@angular/forms": "21.0.0",
    "@angular/material": "21.0.0",
    "@angular/material-date-fns-adapter": "21.0.0",
    "@angular/platform-browser": "21.0.0",
    "@angular/platform-browser-dynamic": "21.0.0",
    "@angular/router": "21.0.0",
    "@angular/service-worker": "21.0.0",
    "chart.js": "4.5.1",
    "cypress-wait-until": "3.0.2",
    "date-fns": "4.1.0",
    "jwt-decode": "4.0.0",
    "ng2-charts": "8.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.0.0",
    "@angular/cli": "21.0.0",
    "@angular/compiler-cli": "21.0.0",
    "@eslint/js": "9.39.1",
    "@types/node": "24.10.1",
    "cypress": "15.7.0",
    "cypress-multi-reporters": "2.0.5",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-cypress": "5.2.0",
    "globals": "16.5.0",
    "jsdom": "27.2.0",
    "mochawesome": "7.1.4",
    "mochawesome-merge": "5.1.0",
    "mochawesome-report-generator": "6.3.2",
    "prettier": "3.7.1",
    "stylelint": "16.26.1",
    "stylelint-config-standard-scss": "16.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.0",
    "vitest": "4.0.14"
  }
}
